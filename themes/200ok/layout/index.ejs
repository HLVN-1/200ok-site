        </div>
      </div>
    </div>

    <div class="row">
      <div class="col-lg-12">
        <!-- Conference Schedule -->
        <div id="conference" class="tab-pane fade active in">

          <% site.data.events.forEach(event => { %>
            <% if (event.title === "Lunch Break") { %>
              <%- partial("_partial/schedule-lunch", {event}) %>
            <% } else { %>
              <%- partial("_partial/schedule-event", {event, page: speaker_page(event.speaker)}) %>
            <% } %>
          <% }) %>

        </div>
      </div>
    </div>
  </div>
</section>




